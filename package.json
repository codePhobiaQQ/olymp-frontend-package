    "antd": "^5.22.4",
    "axios": "^1.6.8",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.11",
    "gsap": "^3.13.0",
    "html-react-parser": "^5.2.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
