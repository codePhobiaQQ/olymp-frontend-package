    "pwa-install-handler": "^2.6.2",
    "qrcode": "^1.5.4",
    "react-equal-height": "^1.3.1",
    "react-frame-component": "^5.2.6",
    "react-input-mask": "^2.0.4",
    "react-latex-next": "^3.0.0",
    "react-quiz-component": "^0.8.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-slick": "^0.30.2",
    "react-to-print": "^3.1.0",
    "redux": "^5.0.1",
